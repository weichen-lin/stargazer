generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["postgresqlExtensions"]
}

datasource db {
  provider   = "postgresql"
  url        = env("DATABASE_URL")
  extensions = [uuid_ossp(map: "uuid-ossp", schema: "public"), vector(schema: "public")]
}

model repo_embedding_info {
  id                    String                 @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  repo_id               BigInt                 @unique
  created_at            DateTime               @default(now()) @db.Timestamptz(6)
  updated_at            DateTime               @default(now()) @db.Timestamptz(6)
  deleted_at            DateTime?              @db.Timestamptz(6)
  full_name             String
  avatar_url            String?                @db.VarChar(255)
  html_url              String?                @db.VarChar(255)
  stargazers_count      Int?
  language              String?                @db.VarChar(50)
  default_branch        String?                @db.VarChar(50)
  description           String?
  readme                String?
  description_embedding Unsupported("vector(1536)")?
  readme_embedding      Unsupported("vector(1536)")?
}

model schema_migrations {
  version BigInt  @id
  dirty   Boolean
}
